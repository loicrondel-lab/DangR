#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/vite@6.3.5_@types+node@18.16.9_jiti@2.4.2_less@4.2.0_sass-embedded@1.90.0_sass@1.90.0_s_b86ecd61e8ea253fa68468bb0420cedc/node_modules/vite/bin/node_modules:/workspace/node_modules/.pnpm/vite@6.3.5_@types+node@18.16.9_jiti@2.4.2_less@4.2.0_sass-embedded@1.90.0_sass@1.90.0_s_b86ecd61e8ea253fa68468bb0420cedc/node_modules/vite/node_modules:/workspace/node_modules/.pnpm/vite@6.3.5_@types+node@18.16.9_jiti@2.4.2_less@4.2.0_sass-embedded@1.90.0_sass@1.90.0_s_b86ecd61e8ea253fa68468bb0420cedc/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/vite@6.3.5_@types+node@18.16.9_jiti@2.4.2_less@4.2.0_sass-embedded@1.90.0_sass@1.90.0_s_b86ecd61e8ea253fa68468bb0420cedc/node_modules/vite/bin/node_modules:/workspace/node_modules/.pnpm/vite@6.3.5_@types+node@18.16.9_jiti@2.4.2_less@4.2.0_sass-embedded@1.90.0_sass@1.90.0_s_b86ecd61e8ea253fa68468bb0420cedc/node_modules/vite/node_modules:/workspace/node_modules/.pnpm/vite@6.3.5_@types+node@18.16.9_jiti@2.4.2_less@4.2.0_sass-embedded@1.90.0_sass@1.90.0_s_b86ecd61e8ea253fa68468bb0420cedc/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
