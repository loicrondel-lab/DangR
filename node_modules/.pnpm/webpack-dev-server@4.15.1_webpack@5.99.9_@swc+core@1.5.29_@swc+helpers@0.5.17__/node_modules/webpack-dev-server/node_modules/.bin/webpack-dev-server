#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.99.9_@swc+core@1.5.29_@swc+helpers@0.5.17__/node_modules/webpack-dev-server/bin/node_modules:/workspace/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.99.9_@swc+core@1.5.29_@swc+helpers@0.5.17__/node_modules/webpack-dev-server/node_modules:/workspace/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.99.9_@swc+core@1.5.29_@swc+helpers@0.5.17__/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.99.9_@swc+core@1.5.29_@swc+helpers@0.5.17__/node_modules/webpack-dev-server/bin/node_modules:/workspace/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.99.9_@swc+core@1.5.29_@swc+helpers@0.5.17__/node_modules/webpack-dev-server/node_modules:/workspace/node_modules/.pnpm/webpack-dev-server@4.15.1_webpack@5.99.9_@swc+core@1.5.29_@swc+helpers@0.5.17__/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../bin/webpack-dev-server.js" "$@"
fi
