#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules/vite/bin/node_modules:/workspace/node_modules/.pnpm/vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules/vite/node_modules:/workspace/node_modules/.pnpm/vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules/vite/bin/node_modules:/workspace/node_modules/.pnpm/vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules/vite/node_modules:/workspace/node_modules/.pnpm/vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@5.0.13_@types+node@18.16.9_less@4.2.0_sass@1.90.0_stylus@0.59.0_terser@5.29.1/node_modules/vite/bin/vite.js" "$@"
fi
