#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/jest@29.7.0_@types+node@18.16.9_babel-plugin-macros@3.1.0_ts-node@10.9.1_@swc+core@1.3._4ab5e2849b1dc482ee121dfc00b0aab6/node_modules/jest/bin/node_modules:/workspace/node_modules/.pnpm/jest@29.7.0_@types+node@18.16.9_babel-plugin-macros@3.1.0_ts-node@10.9.1_@swc+core@1.3._4ab5e2849b1dc482ee121dfc00b0aab6/node_modules/jest/node_modules:/workspace/node_modules/.pnpm/jest@29.7.0_@types+node@18.16.9_babel-plugin-macros@3.1.0_ts-node@10.9.1_@swc+core@1.3._4ab5e2849b1dc482ee121dfc00b0aab6/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/jest@29.7.0_@types+node@18.16.9_babel-plugin-macros@3.1.0_ts-node@10.9.1_@swc+core@1.3._4ab5e2849b1dc482ee121dfc00b0aab6/node_modules/jest/bin/node_modules:/workspace/node_modules/.pnpm/jest@29.7.0_@types+node@18.16.9_babel-plugin-macros@3.1.0_ts-node@10.9.1_@swc+core@1.3._4ab5e2849b1dc482ee121dfc00b0aab6/node_modules/jest/node_modules:/workspace/node_modules/.pnpm/jest@29.7.0_@types+node@18.16.9_babel-plugin-macros@3.1.0_ts-node@10.9.1_@swc+core@1.3._4ab5e2849b1dc482ee121dfc00b0aab6/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
