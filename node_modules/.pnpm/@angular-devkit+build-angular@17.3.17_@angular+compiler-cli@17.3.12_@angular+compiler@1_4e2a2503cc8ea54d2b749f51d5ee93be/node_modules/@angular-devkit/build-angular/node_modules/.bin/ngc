#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/src/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/src/bin/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/src/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/node_modules:/workspace/node_modules/.pnpm/@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/src/bin/ngc.js" "$@"
else
  exec node  "$basedir/../../../../../../@angular+compiler-cli@17.3.12_@angular+compiler@17.3.12_typescript@5.4.5/node_modules/@angular/compiler-cli/bundles/src/bin/ngc.js" "$@"
fi
