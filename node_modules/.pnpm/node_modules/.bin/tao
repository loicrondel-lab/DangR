#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/@nrwl+tao@18.3.3_@swc-node+register@1.8.0_@swc+core@1.3.107_@swc+helpers@0.5.17__@swc+t_bd5feb51c41c5512257fe7d312dd8dc2/node_modules/@nrwl/tao/node_modules:/workspace/node_modules/.pnpm/@nrwl+tao@18.3.3_@swc-node+register@1.8.0_@swc+core@1.3.107_@swc+helpers@0.5.17__@swc+t_bd5feb51c41c5512257fe7d312dd8dc2/node_modules/@nrwl/node_modules:/workspace/node_modules/.pnpm/@nrwl+tao@18.3.3_@swc-node+register@1.8.0_@swc+core@1.3.107_@swc+helpers@0.5.17__@swc+t_bd5feb51c41c5512257fe7d312dd8dc2/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/@nrwl+tao@18.3.3_@swc-node+register@1.8.0_@swc+core@1.3.107_@swc+helpers@0.5.17__@swc+t_bd5feb51c41c5512257fe7d312dd8dc2/node_modules/@nrwl/tao/node_modules:/workspace/node_modules/.pnpm/@nrwl+tao@18.3.3_@swc-node+register@1.8.0_@swc+core@1.3.107_@swc+helpers@0.5.17__@swc+t_bd5feb51c41c5512257fe7d312dd8dc2/node_modules/@nrwl/node_modules:/workspace/node_modules/.pnpm/@nrwl+tao@18.3.3_@swc-node+register@1.8.0_@swc+core@1.3.107_@swc+helpers@0.5.17__@swc+t_bd5feb51c41c5512257fe7d312dd8dc2/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nrwl/tao/index.js" "$@"
else
  exec node  "$basedir/../@nrwl/tao/index.js" "$@"
fi
