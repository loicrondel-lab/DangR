#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules/create-jest/bin/node_modules:/workspace/node_modules/.pnpm/create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules/create-jest/node_modules:/workspace/node_modules/.pnpm/create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules/create-jest/bin/node_modules:/workspace/node_modules/.pnpm/create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules/create-jest/node_modules:/workspace/node_modules/.pnpm/create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules/create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../../../../create-jest@29.7.0_@types+node@18.16.9_ts-node@10.9.1_@swc+core@1.3.107_@swc+helpers@0._033460c21c8c5da5d1369e1b92934fb3/node_modules/create-jest/bin/create-jest.js" "$@"
fi
