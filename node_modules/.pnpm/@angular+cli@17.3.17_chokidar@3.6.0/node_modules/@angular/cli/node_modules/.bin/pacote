#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/lib/node_modules:/workspace/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/node_modules:/workspace/node_modules/.pnpm/pacote@17.0.6/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/lib/node_modules:/workspace/node_modules/.pnpm/pacote@17.0.6/node_modules/pacote/node_modules:/workspace/node_modules/.pnpm/pacote@17.0.6/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pacote@17.0.6/node_modules/pacote/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../pacote@17.0.6/node_modules/pacote/lib/bin.js" "$@"
fi
